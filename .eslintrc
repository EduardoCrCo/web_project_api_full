{
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "no-console": "off",
    "import/extensions": ["error", "always"],
    "semi": ["error", "never"],
    "no-useless-escape": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  }
}
